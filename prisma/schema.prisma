generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

model GuildConfigs {
  id               Int      @id @default(autoincrement())
  guildID          String   @unique
  guildName        String
  modLogsIsEnabled Boolean  @default(true)
  modLogsChannelID String?
  musicIsEnabled   Boolean? @default(false)
}
