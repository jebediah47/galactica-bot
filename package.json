{
  "name": "galactica-bot",
  "version": "6.2.5",
  "description": "A open-source Discord bot made with Typescript using the Discord.js v14 library.",
  "main": "/src/main.ts",
  "scripts": {
    "dev": "nodemon",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "clean": "node buildCleanup.mjs",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:planetscale": "prisma db push --schema=./prisma/schema.planetscale.prisma",
    "prisma:sqlite": "prisma db push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jebediah47/galactica-bot.git"
  },
  "author": "jebediah47",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/jebediah47/galactica-bot/issues"
  },
  "homepage": "https://galacticabot.vercel.app/",
  "dependencies": {
    "@iamtraction/google-translate": "^2.0.1",
    "@prisma/client": "^5.0.0",
    "axios": "^1.6.7",
    "discord-tictactoe": "^4.0.0",
    "discord.js": "^14.14.1",
    "figlet": "^1.7.0",
    "mathjs": "^12.4.0",
    "prisma": "^5.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@swc/core": "1.4.6",
    "@swc/helpers": "0.5.6",
    "@types/figlet": "1.5.8",
    "@types/node": "18.19.22",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
