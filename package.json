{
  "name": "galactica-bot",
  "version": "6.2.5",
  "description": "A open-source Discord bot made with Typescript using the Discord.js v14 library.",
  "main": "/src/main.ts",
  "scripts": {
    "dev": "nodemon",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "clean": "node buildCleanup.mjs",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:planetscale": "prisma db push --schema=./prisma/schema.planetscale.prisma",
    "prisma:sqlite": "prisma db push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jebediah47/galactica-bot.git"
  },
  "author": "jebediah47",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/jebediah47/galactica-bot/issues"
  },
  "homepage": "https://galacticabot.vercel.app/",
  "dependencies": {
    "@iamtraction/google-translate": "^2.0.0",
    "@prisma/client": "^4.4.0",
    "axios": "^1.2.1",
    "discord-tictactoe": "^4.0.0",
    "discord.js": "^14.5.0",
    "figlet": "^1.5.2",
    "mathjs": "^11.0.0",
    "prisma": "^4.4.0",
    "tslib": "^2.4.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.83",
    "@swc/helpers": "0.5.2",
    "@types/figlet": "1.5.8",
    "@types/node": "18.16.18",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "nodemon": "2.0.22",
    "prettier": "3.0.3",
    "ts-node": "10.9.1"
  },
  "engines": {
    "node": ">=16.19.0"
  }
}
