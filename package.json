{
  "name": "galactica-bot",
  "version": "6.2.4",
  "description": "A open-source Discord bot made with Typescript using the Discord.js v14 library.",
  "main": "/src/main.ts",
  "scripts": {
    "dev": "nodemon",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "clean": "node buildCleanup.mjs",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:planetscale": "prisma db push --schema=./prisma/schema.planetscale.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jebediah47/galactica-bot.git"
  },
  "author": "jebediah47",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jebediah47/galactica-bot/issues"
  },
  "homepage": "https://galacticabot.vercel.app/",
  "dependencies": {
    "@devraelfreeze/discordjs-pagination": "^2.6.4",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.13.0",
    "@distube/soundcloud": "^1.1.0",
    "@distube/spotify": "^1.2.1",
    "@distube/yt-dlp": "^1.1.1",
    "@iamtraction/google-translate": "^1.1.2",
    "@prisma/client": "^4.4.0",
    "axios": "^1.0.0",
    "discord-tictactoe": "^4.0.0",
    "discord.js": "^14.5.0",
    "distube": "^4.0.0",
    "figlet": "^1.5.2",
    "mathjs": "^11.0.0",
    "prisma": "^4.4.0",
    "tslib": "^2.4.0",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@swc/core": "1.3.8",
    "@swc/helpers": "0.4.12",
    "@types/figlet": "1.5.5",
    "@types/node": "17.0.36",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "ts-node": "10.9.1"
  },
  "engines": {
    "node": "16.17.1"
  }
